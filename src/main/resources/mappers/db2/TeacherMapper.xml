<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"
        "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="icu.sunnyc.demo.mapper.db2.TeacherMapper">
    
    <select id="getTeacherById" resultType="icu.sunnyc.demo.entity.Teacher">
        select *
        from teacher t
        where t.id = #{id}
    </select>

</mapper>